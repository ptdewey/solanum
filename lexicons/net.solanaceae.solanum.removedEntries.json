{
  "lexicon": 1,
  "id": "net.solanaceae.solanum.removedEntries",
  "defs": {
    "main": {
      "type": "record",
      "description": "Removed feed entries stored as a blob for filtering.",
      "key": "literal:self",
      "record": {
        "type": "object",
        "required": ["blob", "lastUpdated", "entryCount"],
        "properties": {
          "blob": {
            "type": "blob",
            "accept": ["application/json"],
            "maxSize": 52428800,
            "description": "JSON blob containing removed feed entry URLs."
          },
          "lastUpdated": {
            "type": "string",
            "format": "datetime",
            "description": "Timestamp when the list was last updated."
          },
          "entryCount": {
            "type": "integer",
            "minimum": 0,
            "description": "Number of removed entries in the list."
          }
        }
      }
    }
  }
}
